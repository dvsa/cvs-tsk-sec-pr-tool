{
  "name": "cvs-tsk-sec-pr-tool",
  "version": "2.0.1",
  "description": "Finds dependabot PRs and take actions",
  "main": "index.js",
  "author": "Martin Kemp",
  "repository": "https://github.com/dvsa/cvs-tsk-sec-pr-tool",
  "license": "MIT",
  "engines": {
    "node": ">=12.16 <13"
  },
  "scripts": {
    "audit-dep": "npm audit",
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/index.js",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "test": "npm run lint",
    "test-i": "echo 'Not implemented'",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "clean": "rm -rf lib/",
    "package": "npm run build && docker build -t cvs/sec-pr-tool:$npm_package_version -t cvs/sec-pr-tool:latest .",
    "sonar-scanner": "echo 'Not implemented'"
  },
  "dependencies": {
    "jenkins": "^0.28.0",
    "probot": "^9.11.5"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/jenkins": "^0.23.1",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.x",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "smee-client": "^1.1.0",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6",
    "lodash": "4.17.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
